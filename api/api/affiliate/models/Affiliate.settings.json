{
  "connection": "default",
  "collectionName": "affiliate",
  "info": {
    "name": "affiliate",
    "description": "Site affiliate logos to display on home page"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "order": {
      "unique": true,
      "type": "integer",
      "required": true,
      "min": 1
    }
  }
}